{
    "name": "drupal/dcache",
    "type": "drupal-module",
    "authors": [
        {
            "name": "Merlin",
            "email": "merlin@geeks4change.net"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "autoload": {
        "psr-4": {
            "Drupal\\dcache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\dcache\\": "tests/src/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "drush/drush": "^11.3",
        "drupal/core-dev": "^9.4 || ^10.0",
        "drupal/core": "^9.4 || ^10.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupalspoons/composer-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
